oTravel == null || this.regioneTravel == null){\n alert(\"Compila tutti i campi\") \n}else{\nthis.$store.commit('a/addFilt',{mese: this.meseTravel, durata: this.durataTravel, prezzo: this.  prezzoTravel, regione: this.regioneTravel}) \n}\nthis.meseTravel = null\nthis.durataTravel = null\nthis.prezzoTravel = null\nthis.regioneTravel = null\n\n},\n    \n\n    \nchangeFalse(){\nif(this. meseTravel == null || this.durataTravel == null || this.prezzoTravel == null || this.regioneTravel == null){\nreturn;    \n}else{\nthis.$store.commit('a/changeButton')} \n},   \n        \nactNome(){\nthis.filterNome = !this.filterNome    \n    },\n        \n    goHome(){\n //this.$router.push('/blog')\nthis.$router.go(this.$router.currentRoute)\n},    \n    }\n    \n}\n\n</script>\n\n\n\n<style scoped>\n    \n  \n    \n    .btn1{\n     background: #043D7A;\n    padding: 10px 10px;\n    color: white;\n    border: 0px;\n    }\n    \n      .btn1:hover, .btn1.active{\n        color: white;\n     background: #0556ad;\n    \n    }\n    \n    .btn2{\n     background: #a09f9f;\n    padding: 10px 10px;\n    color: white;\n    border: 0px;\n    }\n    \n       .btn2:hover, .btn2.active{\n     background: #b7b7b7;\n    padding: 10px 10px;\n    color: white;\n    border: 0px;\n     background: #b7b7b7;\n    }\n    \n\n    \n    .btnBig{\n    background: #F0A22A;\n    padding: 10px 10px;\n    border: 0px;\n    font-family: \"fatfrank\", sans-serif;\n    font-size: 20px;\n    }\n    \n    \n      .btnBig:hover, .btnBig.active{\n    background: #f7af40;\n    padding: 10px 10px;\n    border: 0px;\n    }\n    \n    \n    \n    .paddR{\n    padding-right: 0px;    \n    }\n    \n    .MarginL{\n        margin-left: 8px;\n    }\n    \n    .boxFilter{\n    background: rgba(0, 0, 0, 0.81);\n   z-index: 5000;\n   position: fixed;\n   top: 0px;\n    height: 100%;\n    \n    }\n    \n    .resizeBoxFilter{\nborder: 2px solid #043D7A;\n       \n    }\n    \n    \n/* input number senza frecce */\n\ninput::-webkit-o